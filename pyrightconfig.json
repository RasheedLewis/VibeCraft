{
  "include": [
    "backend",
    "scripts"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/venv",
    "**/.venv",
    "**/dist",
    "**/build"
  ],
  "extraPaths": [
    "backend"
  ],
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}

